<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="urn:bpel.travelgood.dtu.dk" xmlns:ns2="urn:niceview.dtu.dk" xmlns:ns3="urn:lameduck.dtu.dk" xmlns:ns4="urn:travelgood.dtu.dk" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>TravelGoodApp</name>
            <description>Represents the Service Assembly of TravelGoodApp</description>
        </identification>
        <service-unit>
            <identification>
                <name>TravelGoodApp-TravelGoodBpel</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>TravelGoodBpel.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>TravelGoodApp-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="niceViewPortTypeRole_partnerRole" service-name="ns1:NiceViewPartnerLink"/>
                <provider endpoint-name="niceViewPortTypeBindingPort" service-name="ns2:niceViewService"/>
            </connection>
            <connection>
                <consumer endpoint-name="lameDuckPortTypeRole_partnerRole" service-name="ns1:LameDuckPartnerLink"/>
                <provider endpoint-name="lameDuckPort" service-name="ns3:lameDuckService"/>
            </connection>
            <connection>
                <consumer endpoint-name="travelGoodPortTypeBindingPort" service-name="ns4:travelGoodService"/>
                <provider endpoint-name="travelGoodPortTypeRole_myRole" service-name="ns1:TravelGoodPartnerLink"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
